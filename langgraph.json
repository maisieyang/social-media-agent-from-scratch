{
  "$schema": "https://langchain-ai.github.io/langgraph/schemas/langgraph.config.schema.json",
  "dependencies": ["."],
  "graphs": {
    "generate_post": "./src/agents/generate-post/graph.ts:generatePostGraph",
    "upload_post": "./src/agents/upload-post/graph.ts:uploadPostGraph",
    "verify_links": "./src/agents/verify-links/graph.ts:verifyLinksGraph"
  },
  "env": ".env",
  "dockerfile_lines": [
    "RUN apt-get update && apt-get install -y chromium",
    "RUN npx playwright install-deps chromium"
  ],
  "store": {
    "type": "memory"
  }
}
